    <!-- ヘッダー -->
    <header class="header">
      <div class="app-name"><a href="#">App Name</a></div>
      <ul class="header-lists">
        <li class="log-in"><a href="#">ログイン</a></li>
        <li class="sign-up"><a href="#">新規登録</a></li>
      </ul>
    </header>

    <!-- ログイン -->
    <div class="contents-row">
      <div class="container-session">
        <%= form_with model: @user, url: user_session_path, id: 'new_user', class: 'new_user', local: true do |f| %>
          <div class="field">
            <%= f.label :メールアドレス %><br />
            <%= f.email_field :email, autofocus: true %>
          </div>
          <div class="field">
            <%= f.label :パスワード %><br />
            <%= f.password_field :password, autocomplete: "off" %>
          </div>
          <div class="field">
            <%= f.submit "ログイン" %>
          </div>
        <% end %>
      </div>
    </div>
